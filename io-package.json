    "encryptedNative": [
        "password"
    ],
    "protectedNative": [
        "password"
    ],
    "native": {
        "email": "",
        "password": "",
        "deviceUpdateSeconds": 5,
        "generalUpdateSeconds": 10,
        "onWattThreshold": 2,
        "deviceFilter": []
    },
    "objects": [],
    "instanceObjects": [
        {
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "role": "indicator.connected",
                "name": "Device or service connected",
                "type": "boolean",
                "read": true,
                "write": false,
                "def": false
            },
            "native": {}
        },
        {
            "_id": "grid",
            "type": "channel",
            "common": {
                "name": "Sense Power Grid Metrics"
            },
            "native": {}
        },
        {
            "_id": "grid.frequency",
            "type": "state",
            "common": {
                "role": "value",
                "name": "Grid Frequency",
                "type": "number",
                "unit": "Hz",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "grid.wattage",
            "type": "state",
            "common": {
                "role": "value",
                "name": "Grid Power Consumption",
                "type": "number",
                "unit": "W",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "grid.wattageL1",
            "type": "state",
            "common": {
                "role": "value",
                "name": "Grid Power Consumption (Leg 1)",
                "type": "number",
                "unit": "W",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "grid.wattageL2",
            "type": "state",
            "common": {
                "role": "value",
                "name": "Grid Power Consumption (Leg 2)",
                "type": "number",
                "unit": "W",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "grid.voltageL1",
            "type": "state",
            "common": {
                "role": "value",
                "name": "Grid Voltage (Leg 1)",
                "type": "number",
                "unit": "V",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "grid.voltageL2",
            "type": "state",
            "common": {
                "role": "value",
                "name": "Grid Voltage (Leg 2)",
                "type": "number",
                "unit": "V",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "devices",
            "type": "folder",
            "common": {
                "name": "Sense Device Metrics"
            },
            "native": {}
        }
